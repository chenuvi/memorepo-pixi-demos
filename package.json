{
  "name": "memorepo-pixi-demos",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:all": "pnpm -r --filter=!main-app build",
    "build:netlify": "node build-netlify.js",
    "dev:fish-pond": "pnpm --filter fish-pond dev",
    "dev:choo-choo": "pnpm --filter choo-choo-train dev",
    "dev:spine": "pnpm --filter spine-boy-adventure dev",
    "dev:all": "concurrently \"pnpm --filter fish-pond dev\" \"pnpm --filter choo-choo-train dev\" \"pnpm --filter spine-boy-adventure dev\"",
    "install:all": "pnpm -r install",
    "vercel:deploy:fish-pond": "vercel --scope pnpm --prod --cwd ./apps/fish-pond",
    "vercel:deploy:choo-choo": "vercel --scope pnpm --prod --cwd ./apps/choo-choo-train",
    "vercel:deploy:spine": "vercel --scope pnpm --prod --cwd ./apps/spine-boy-adventure",
    "vercel:deploy:all": "pnpm vercel:deploy:fish-pond && pnpm vercel:deploy:choo-choo && pnpm vercel:deploy:spine",
    "netlify:deploy:fish-pond": "cd apps/fish-pond && netlify deploy --prod",
    "netlify:deploy:choo-choo": "cd apps/choo-choo-train && netlify deploy --prod",
    "netlify:deploy:spine": "cd apps/spine-boy-adventure && netlify deploy --prod",
    "netlify:deploy:all": "pnpm netlify:deploy:fish-pond && pnpm netlify:deploy:choo-choo && pnpm netlify:deploy:spine"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  }
}
